        <div class="back">
        <div class="container">
            <div id="not-drag">
          <div class="formulaire">   
        <form action="add" method="post">
            
                <label for="add">add contact</label><br>
                <label>Id</label><input type="text" name="id" value=""/>
                <label>Firstname</label><input type="text" name="firstName" value=""/>
                <label>Lastname</label><input type="text" name="lastName" value=""/>
                <label>phone</label><input type="text" name="phone" value=""/>
                <label>email</label><input type="text" name="email" value=""/>
                <input type="submit" value="valider"/>  
        </form>
        </div>
        </div>
            </div>
    
    <div class="container">
   
    <div class=row>     
        <% 
        for(var i = 0; i < clientList.length; i++) {
        %>
  
    <div class="box">
     
    <div class="col-md-4">
        <div id="draggable" ><!--class="ui-widget-content">-->
        <div class="contain">    
                <h2><%= clientList[i].id+" "+clientList[i].firstName+" "+clientList[i].lastName %></h2>
                <p><%= clientList[i].phone %></p>            
                <p><%= clientList[i].email %></p>
               <!--   <div align="center">  -->
<div>
                <form action="delete" method="get" value="delete" >
                     <input name="id" value="<%= clientList[i].id %>" hidden="true">
                     <button class="btn btn-success"  type="submit">delete</button> 
                </form>

                <form action="update" method="get" value="update">
                     <input name="id" value="<%= clientList[i].id %>" hidden="true">
                     <button class="btn btn-success" type="submit">update</button>
                </form>
</div>    
            </div>
        </div>
        
        </div>
          <%     
        } 
